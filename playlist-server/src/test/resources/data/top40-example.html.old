	<script type="text/javascript">
		var sActiveTrend = '';
		jQuery(function() {
			sCookieContent = '7032' + "###" + '/top40/2018/week-11' + "###"
					+ 'Top 40 week 11 van 2018';
			setCookie('top40_dynnav', sCookieContent, 60, '/', '', '');
			setCookie('top40_dynnav_prevpage', 'chart_detail', 5, '/', '', '');

			jQuery('.trend-cell a')
					.click(
							function(e) {
								e.preventDefault();
								var sTrend = jQuery(this).data('trend');

								if (!jQuery(this).hasClass('active')
										&& sTrend != '') {
									sActiveTrend = sTrend;

									jQuery('.trend-cell a.active').removeClass(
											'active');
									jQuery('.chartList > div')
											.not('.' + sTrend).slideUp();
									jQuery('.chartList > div.' + sTrend)
											.slideDown();

									$('#dropdownMenuButton').html(
											$(this).html());

									$('#chart-list .ad').attr('style',
											'display:none!important');

									jQuery(this).addClass('active');
									ga('send', 'event', 'trends', sTrend,
											'Top 40 week 11 van 2018 (17-03-2018)');
								} else {
									$('#dropdownMenuButton').html('Trends');
									$('#chart-list .ad').attr('style',
											'display:block');
									resetTrends();
								}
								return false;
							});
		});

		function resetTrends() {
			jQuery('.trend-cell a.active').removeClass('active');
			$('.chartList > div').slideDown();

			$('.chartList > div.no-longer-listed').hide();
		}

		window.addEventListener("DOMContentLoaded", parallax, false);

